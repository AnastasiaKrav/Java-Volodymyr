import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner num = new Scanner(System.in);
        int i;
        i = num.nextInt();
        System.out.println("My number is: " + i);

        Scanner str = new Scanner(System.in);
        String s;
        s = str.nextLine();
        System.out.println("My text is: " + s);
    }
}
